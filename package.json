{
  "name": "patternity-evo",
  "repository": "https://github.com/influitive/patternity-evo",
  "version": "0.0.7",
  "description": "Component Library for Influitive",
  "main": "index.js",
  "scripts": {
    "test:watch": "nodemon --quiet --exec \"npm run --silent test\" --watch src",
    "test:ci": "BABEL_ENV=test babel-tape-runner \"src/**/*/*test.js\" | tap-dot",
    "ci": "npm run test:ci && npm run lint",
    "test": "BABEL_ENV=test tape-babel-css-modules \"src/**/*/*test.js\" | tap-spec",
    "build": "rm -rf lib && gulp --silent && babel --quiet src -d lib",
    "pub": "NODE_ENV=production npm run build && npm publish",
    "start": "kotatsu serve -c example/webpack.config.js --progress --devtool \\#eval-source-map --babel example/index.js & npm run test:watch",
    "lint": "eslint ."
  },
  "keywords": [
    "React",
    "Component",
    "postcss",
    "jss"
  ],
  "author": "SeeThruHead",
  "license": "MIT",
  "dependencies": {
    "color": "0.11.1",
    "jss": "3.3.0",
    "jss-nested": "1.0.2",
    "react-bootstrap": "0.28.3",
    "react-jss": "2.0.1",
    "react-select": "^1.0.0-beta8"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-eslint": "4.1.8",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.5.2",
    "babel-tape-runner": "2.0.0",
    "css-loader": "0.23.1",
    "devcards": "git://github.com/glenjamin/devcards-js.git",
    "enzyme": "2.0.0",
    "eslint": "1.10.3",
    "eslint-plugin-filenames": "0.2.0",
    "eslint-plugin-react": "4.2.2",
    "express": "4.13.4",
    "file-loader": "0.8.5",
    "glob": "7.0.0",
    "glob-fs": "0.1.6",
    "gulp": "3.9.1",
    "gulp-postcss": "6.1.0",
    "isparta": "4.0.0",
    "kotatsu": "^0.10.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "postcss-functions": "2.1.0",
    "postcss-import": "8.0.2",
    "postcss-loader": "0.8.0",
    "postcss-media-minmax": "2.1.1",
    "postcss-nested": "1.0.0",
    "postcss-simple-vars": "1.2.0",
    "react": "0.14.7",
    "react-addons-test-utils": "0.14.7",
    "react-dom": "0.14.7",
    "style-loader": "0.13.0",
    "tap-dot": "1.0.1",
    "tap-spec": "4.1.1",
    "tape": "4.4.0",
    "tape-babel-css-modules": "^1.2.1",
    "tape-catch": "1.0.4",
    "url-loader": "0.5.7",
    "webpack": "1.12.13"
  }
}
